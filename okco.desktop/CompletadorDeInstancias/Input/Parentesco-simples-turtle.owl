#################################################################


###  http://www.semanticweb.org/parentesco-simples.owl#isTheFatherOf

:isTheFatherOf rdf:type owl:ObjectProperty .



###  http://www.semanticweb.org/parentesco-simples.owl#isTheSonOf

:isTheSonOf rdf:type owl:ObjectProperty ;
            
            owl:inverseOf :isTheFatherOf .



###  http://www.semanticweb.org/parentesco-simples.owl#outra

:outra rdf:type owl:ObjectProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.semanticweb.org/parentesco-simples.owl#Father

:Father rdf:type owl:Class ;
        
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :isTheFatherOf ;
                              owl:someValuesFrom :Son
                            ] .



###  http://www.semanticweb.org/parentesco-simples.owl#Son

:Son rdf:type owl:Class ;
     
     owl:equivalentClass [ rdf:type owl:Restriction ;
                           owl:onProperty :isTheSonOf ;
                           owl:someValuesFrom :Father
                         ] ;
     
     rdfs:subClassOf owl:Thing .





#################################################################
#
#    Individuals
#
#################################################################


###  http://www.semanticweb.org/parentesco-simples.owl#f1

:f1 rdf:type :Father ,
             owl:NamedIndividual ;
    
    :isTheFatherOf :s1 .



###  http://www.semanticweb.org/parentesco-simples.owl#f2

:f2 rdf:type :Father ,
             owl:NamedIndividual ;
    
    owl:sameAs :f3 .



###  http://www.semanticweb.org/parentesco-simples.owl#f3

:f3 rdf:type :Father ,
             owl:NamedIndividual .



###  http://www.semanticweb.org/parentesco-simples.owl#s1

:s1 rdf:type :Son ,
             owl:NamedIndividual .




